{
  "name": "kraken-bot",
  "version": "0.1.0",
  "description": "A Discord bot for Squid's server",
  "private": true,
  "author": "spuggle",
  "license": "Apache-2.0",
  "homepage": "https://github.com/spuggle/kraken-bot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spuggle/kraken-bot.git"
  },
  "keywords": [
    "discord",
    "chatbot",
    "discord.js"
  ],
  "bugs": {
    "url": "https://github.com/spuggle/kraken-bot/issues"
  },
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --pretty --sourceMap --watch",
    "lint": "eslint src/",
    "lint:commit": "git log -1 --pretty=format:'%B' | commitlint",
    "lint:fix": "eslint --fix src/",
    "postinstall": "tsc",
    "start": "node dist/index.js",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@twurple/api": "^5.2.3",
    "@twurple/auth": "^5.2.3",
    "@twurple/eventsub": "^5.2.3",
    "chalk": "^5.0.1",
    "discord-api-types": "^0.37.8",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "tmi.js": "^1.8.5",
    "unique-random-array": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.7.16",
    "@types/tmi.js": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "tslib": "^2.4.0",
    "type-fest": "^2.19.0",
    "typescript": "^4.8.3"
  },
  "commitlint": {
    "extends": [
      "./.github/configs/commitlint.json"
    ]
  },
  "eslintConfig": {
    "extends": "./.eslint/all.json",
    "root": true,
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2022
    }
  }
}
