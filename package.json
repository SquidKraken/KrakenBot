{
  "name": "kraken-bot",
  "version": "0.1.0",
  "description": "A Discord bot for Squid's server",
  "private": true,
  "author": "spuggle",
  "license": "Apache-2.0",
  "homepage": "https://github.com/spuggle/kraken-bot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spuggle/kraken-bot.git"
  },
  "keywords": [
    "discord",
    "chatbot",
    "discord.js"
  ],
  "bugs": {
    "url": "https://github.com/spuggle/kraken-bot/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm run build:patch; tsc",
    "build:patch": "pnpx ts-patch install --silent",
    "build:watch": "pnpm run build:patch; tsc --pretty --sourceMap --watch",
    "lint": "pnpx eslint src/",
    "lint:commit": "git log -1 --pretty=format:'%B' | commitlint",
    "lint:fix": "pnpx eslint --fix src/",
    "prepare": "pnpx husky install",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^8.0.1",
    "ts-patch": "^2.0.1",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "type": "module",
  "commitlint": {
    "extends": [
      "./.github/configs/commitlint.json"
    ]
  },
  "eslintConfig": {
    "extends": "./.eslint/all.json",
    "root": true,
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2021
    }
  },
  "dependencies": {
    "discord.js": "^13.8.0"
  }
}
